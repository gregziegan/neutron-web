{
  "extends": "airbnb",
  "globals": {
    "__DEV__"      : false,
    "__PROD__"     : false,
    "__DEBUG__"    : false,
    "__DEBUG_NW__" : false
  },
  "rules": {
    "comma-dangle": [0, "never"],
    "space-before-function-paren": [0, "always"],
    "react/jsx-quotes": [0, "single"],
    "no-use-before-define": 0
  }
}
